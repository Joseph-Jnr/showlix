{
  "expo": {
    "name": "Showlix",
    "slug": "showlix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#151312",
        "foregroundImage": "./assets/icons/logo.png",
        "monochromeImage": "./assets/icons/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.josephjnr.showlix"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/logo.png",
          "imageWidth": 140,
          "resizeMode": "contain",
          "backgroundColor": "#151312"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38520694-7f4e-4bf2-8e4d-7fae8c330777"
      }
    },
    "owner": "josephjnr",
    "jsEngine": "hermes"
  }
}
